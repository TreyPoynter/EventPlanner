@model Event
@{
    ViewBag.Title = "Current Events";
}

<div class="jumbotron-container mb-3" id="jumbotron">
    <div class="jumbotron-background balloons"></div>
    <div class="jumbotron-overlay"></div>
    <div class="jumbotron-text">
        <p class="display-2">View Events Here</p>
        <form>
            <div class="searchBox">
                <input class="searchInput" type="text" name="" placeholder="Search for a great time">
                <button class="searchButton" href="#">
                    <i class="fa-solid fa-magnifying-glass"></i>
                </button>
            </div>
        </form>
    </div>
</div>
